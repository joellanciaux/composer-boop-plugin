{
  "name": "cursor-beep",
  "displayName": "Cursor Beep",
  "description": "VS Code extension to interact with Cursor composer text box",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cursor-beep.interact",
        "title": "Interact with Cursor Composer"
      },
      {
        "command": "cursor-beep.accept",
        "title": "Accept Cursor Changes"
      }
    ],
    "keybindings": [
      {
        "command": "cursor-beep.accept",
        "key": "cmd+y",
        "mac": "cmd+y",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2"
  }
}
